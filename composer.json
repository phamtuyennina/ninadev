{
  "name": "ninadev/php",
  "type": "project",
  "description": "A framework built by Nina.",
  "keywords": ["nina", "framework"],
  "license": "MIT",
  "autoload": {
    "files": [
      "src/Core/functions.php"
    ],
    "psr-4": {
      "NINA\\Core\\": "src/Core/",
      "NINA\\Traits\\": "src/Core/database/Traits/",
      "NINA\\Database\\": "src/Core/database/",
      "Pecee\\": "src/Core/Pecee/"
    }
  },
  "require": {
    "php": "^8.1",
    "symfony/var-dumper": "^7.0",
    "illuminate/http": "^11.0",
    "jenssegers/agent": "^2.6",
    "illuminate/container": "^11.1",
    "symfony/cache": "^7.0",
    "league/commonmark": "^2.2.1",
    "league/flysystem": "^3.8.0",
    "ext-pdo": "*",
    "ext-fileinfo": "*",
    "ext-ctype": "*",
    "ext-filter": "*",
    "ext-hash": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-session": "*",
    "ext-tokenizer": "*",
    "illuminate/filesystem": "^11.3",
    "intervention/image": "^3.5"
  },
  "require-dev": {
    "spatie/ignition": "^1.13"
  }
}
